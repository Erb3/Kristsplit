	"description": "Redirect your krist with ease, with Kristsplit",
	"main": "src/index.ts",
	"keywords": ["krist", "switchcraft", "cc-tweaked"],
	"homepage": "https://github.com/Erb3/Kristsplit",
	"bugs": {
		"url": "https://github.com/Erb3/Kristsplit/issues"
	},
	"license": "MPL-2.0",
